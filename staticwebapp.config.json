{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/robots.txt", "/sitemap.xml", "/*.{css,js,woff,woff2,ttf,eot,svg,png,jpg,jpeg,gif,ico}"]
    },
    "routes": [
        {
            "route": "/robots.txt",
            "serve": "/robots.txt",
            "statusCode": 200
        },
        {
            "route": "/sitemap.xml",
            "serve": "/sitemap.xml",
            "statusCode": 200
        }
    ],
    "mimeTypes": {
        ".json": "application/json"
    }
}
