  "scripts": {
    "ng": "ng",
    "start": "node scripts/load-env.js && ng serve",
    "build": "ng build",
    "build:prod": "cross-env NODE_ENV=production node scripts/set-env.js && ng build --configuration=production",
    "setup:env": "node scripts/load-env.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:tailwind": "tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css",
    "install:clean": "npx rimraf node_modules package-lock.json && npm install"
  },
  "dependencies": {
